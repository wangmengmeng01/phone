{
  "version": "1.0",
  "vconsole": false,
  "state": {
    "header": {
      "token": ""
    }
  },
  "route": [
    {
      "path": "/",
      "redirect": "/webapp"
    },
    {
      "path": "login",
      "title": "登录",
      "filter": true,
      "back": {
        "title": false
      },
      "foot": {
        "type": "main",
        "style": "padding-bottom: 1rem"
      }
    },
    {
      "path": "register",
      "title": "注册",
      "filter": true,
      "foot": {
        "type": "main",
        "style": "padding-bottom: 1rem"
      }
    },
    {
      "path": "set_pwd",
      "title": "注册",
      "filter": true,
      "foot": {
        "type": "main",
        "style": "padding-bottom: 1rem"
      }
    },
    {
      "path": "forget_pwd",
      "title": "修改登录密码",
      "filter": true,
      "back": {
        "showtip": true,
        "title": false
      },
      "foot": false
    },
    {
      "path": "reg_bank",
      "title": "开通银行存管账户",
      "filter": true,
      "foot": {
        "type": "main",
        "style": "padding-bottom: 1rem"
      }
    },
    {
      "path": "webapp",
      "component": "main",
      "children": [
        {
          "path": "",
          "redirect": "home"
        },
        {
          "path": "home",
          "title": "首页",
          "filter": true,
          "back": false,
          "foot": false
        },
        {
          "path": "product",
          "title": "产品",
          "back": {
            "icon": false,
            "background": "main"
          },
          "foot": false
        },
        {
          "path": "wealth",
          "title": "财富",
          "back": {
            "icon": false,
            "background": "main"
          }
        },
        {
          "path": "found",
          "title": "发现",
          "back": {
            "icon": false,
            "background": "main"
          }
        },
        {
          "path": "mine",
          "title": "我的",
          "back": {
            "icon": false,
            "background": "main"
          }
        }
      ]
    },
    {
      "path": "mine",
      "component": "mine",
      "children": [
        {
          "path": "",
          "redirect": "index"
        },
        {
          "path": "qrcode",
          "title": "我的二维码"
        },
        {
          "path": "customer",
          "title": "已有客户"
        },
        {
          "path": "customer_detail",
          "title": "已有客户详情"
        },
        {
          "path": "send_discount",
          "title": "赠送优惠"
        }
      ]
    },
    {
      "path": "static",
      "component": "static",
      "children": [
        {
          "path": "",
          "redirect": "succ"
        },
        {
          "path": "succ",
          "title": "成功"
        }
      ]
    },
    {
      "path": "*",
      "component": "notfound",
      "title": "404"
    }
  ]
}
